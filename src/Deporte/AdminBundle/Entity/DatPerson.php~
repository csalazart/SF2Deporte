<?php

namespace Deporte\AdminBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * DatPerson
 *
 * @ORM\Table(name="dat_person")
 * @ORM\Entity(repositoryClass="Deporte\AdminBundle\Entity\DatPersonRepository")
 */
class DatPerson
{
    /**
     * @var string
     *
     * @ORM\Column(name="cedula", type="string", length=9, nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $cedula;

    /**
     * @var string
     *
     * @ORM\Column(name="apellido1", type="string", length=64, nullable=false)
     */
    private $apellido1;

    /**
     * @var string
     *
     * @ORM\Column(name="apellido2", type="string", length=64, nullable=false)
     */
    private $apellido2;

    /**
     * @var string
     *
     * @ORM\Column(name="nombre1", type="string", length=64, nullable=false)
     */
    private $nombre1;

    /**
     * @var string
     *
     * @ORM\Column(name="nombre2", type="string", length=64, nullable=false)
     */
    private $nombre2;

    /**
     * @var string
     *
     * @ORM\Column(name="tlfhab", type="string", length=16, nullable=false)
     */
    private $tlfhab;

    /**
     * @var string
     *
     * @ORM\Column(name="tlfcel", type="string", length=16, nullable=false)
     */
    private $tlfcel;

    /**
     * @var string
     *
     * @ORM\Column(name="sexo", type="string", length=1, nullable=false)
     */
    private $sexo;

    /**
     * @var string
     *
     * @ORM\Column(name="fechanac", type="string", length=12, nullable=false)
     */
    private $fechanac;

    /**
     * @var string
     *
     * @ORM\Column(name="ciudad", type="string", length=64, nullable=false)
     */
    private $ciudad;

    /**
     * @var string
     *
     * @ORM\Column(name="dir", type="string", length=300, nullable=false)
     */
    private $dir;

    /**
     * @var string
     *
     * @ORM\Column(name="email", type="string", length=64, nullable=false)
     */
    private $email;

    /**
     * @var string
     *
     * @ORM\Column(name="instituto", type="string", length=150, nullable=false)
     */
    private $instituto;



    /**
     * Get cedula
     *
     * @return string 
     */
    public function getCedula()
    {
        return $this->cedula;
    }

    /**
     * Set apellido1
     *
     * @param string $apellido1
     * @return DatPerson
     */
    public function setApellido1($apellido1)
    {
        $this->apellido1 = $apellido1;
    
        return $this;
    }

    /**
     * Get apellido1
     *
     * @return string 
     */
    public function getApellido1()
    {
        return $this->apellido1;
    }

    /**
     * Set apellido2
     *
     * @param string $apellido2
     * @return DatPerson
     */
    public function setApellido2($apellido2)
    {
        $this->apellido2 = $apellido2;
    
        return $this;
    }

    /**
     * Get apellido2
     *
     * @return string 
     */
    public function getApellido2()
    {
        return $this->apellido2;
    }

    /**
     * Set nombre1
     *
     * @param string $nombre1
     * @return DatPerson
     */
    public function setNombre1($nombre1)
    {
        $this->nombre1 = $nombre1;
    
        return $this;
    }

    /**
     * Get nombre1
     *
     * @return string 
     */
    public function getNombre1()
    {
        return $this->nombre1;
    }

    /**
     * Set nombre2
     *
     * @param string $nombre2
     * @return DatPerson
     */
    public function setNombre2($nombre2)
    {
        $this->nombre2 = $nombre2;
    
        return $this;
    }

    /**
     * Get nombre2
     *
     * @return string 
     */
    public function getNombre2()
    {
        return $this->nombre2;
    }

    /**
     * Set tlfhab
     *
     * @param string $tlfhab
     * @return DatPerson
     */
    public function setTlfhab($tlfhab)
    {
        $this->tlfhab = $tlfhab;
    
        return $this;
    }

    /**
     * Get tlfhab
     *
     * @return string 
     */
    public function getTlfhab()
    {
        return $this->tlfhab;
    }

    /**
     * Set tlfcel
     *
     * @param string $tlfcel
     * @return DatPerson
     */
    public function setTlfcel($tlfcel)
    {
        $this->tlfcel = $tlfcel;
    
        return $this;
    }

    /**
     * Get tlfcel
     *
     * @return string 
     */
    public function getTlfcel()
    {
        return $this->tlfcel;
    }

    /**
     * Set sexo
     *
     * @param string $sexo
     * @return DatPerson
     */
    public function setSexo($sexo)
    {
        $this->sexo = $sexo;
    
        return $this;
    }

    /**
     * Get sexo
     *
     * @return string 
     */
    public function getSexo()
    {
        return $this->sexo;
    }

    /**
     * Set fechanac
     *
     * @param string $fechanac
     * @return DatPerson
     */
    public function setFechanac($fechanac)
    {
        $this->fechanac = $fechanac;
    
        return $this;
    }

    /**
     * Get fechanac
     *
     * @return string 
     */
    public function getFechanac()
    {
        return $this->fechanac;
    }

    /**
     * Set ciudad
     *
     * @param string $ciudad
     * @return DatPerson
     */
    public function setCiudad($ciudad)
    {
        $this->ciudad = $ciudad;
    
        return $this;
    }

    /**
     * Get ciudad
     *
     * @return string 
     */
    public function getCiudad()
    {
        return $this->ciudad;
    }

    /**
     * Set dir
     *
     * @param string $dir
     * @return DatPerson
     */
    public function setDir($dir)
    {
        $this->dir = $dir;
    
        return $this;
    }

    /**
     * Get dir
     *
     * @return string 
     */
    public function getDir()
    {
        return $this->dir;
    }

    /**
     * Set email
     *
     * @param string $email
     * @return DatPerson
     */
    public function setEmail($email)
    {
        $this->email = $email;
    
        return $this;
    }

    /**
     * Get email
     *
     * @return string 
     */
    public function getEmail()
    {
        return $this->email;
    }

    /**
     * Set instituto
     *
     * @param string $instituto
     * @return DatPerson
     */
    public function setInstituto($instituto)
    {
        $this->instituto = $instituto;
    
        return $this;
    }

    /**
     * Get instituto
     *
     * @return string 
     */
    public function getInstituto()
    {
        return $this->instituto;
    }
}
